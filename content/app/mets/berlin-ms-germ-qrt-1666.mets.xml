                            <addName xmlns="http://www.tei-c.org/ns/1.0">Aegyptus</addName>
                            <addName xmlns="http://www.tei-c.org/ns/1.0">Aigyptos</addName>
                            <addName xmlns="http://www.tei-c.org/ns/1.0">Hwt-ka-Ptah</addName>
                            <addName xmlns="http://www.tei-c.org/ns/1.0">Kem</addName>
                            <addName xmlns="http://www.tei-c.org/ns/1.0">Kemi</addName>
                            <addName xmlns="http://www.tei-c.org/ns/1.0">Kermit</addName>
                            <addName xmlns="http://www.tei-c.org/ns/1.0">Kemet</addName>
                            <addName xmlns="http://www.tei-c.org/ns/1.0">Kmt</addName>
                            <addName xmlns="http://www.tei-c.org/ns/1.0">Ancient Egyptian</addName><!--//End of metadata enrichment-->
                        </TEI:placeName>
                    </TEI:emph>.</TEI:p>
                <TEI:lb id="d10e1377" n="34"/>
                <TEI:p id="d10e1380">
                    <TEI:hi rend="underline">Bücher darüber </TEI:hi>
                    <TEI:bibl>
                        <TEI:ref target="#http://www.zvdd.de/dms/load/met/?PPN=oai%3Adiglib.hab.de%3Appn_656251174">
