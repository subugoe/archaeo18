                        <addName xmlns="http://www.tei-c.org/ns/1.0">Chassala</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Chassela</addName><!--//End of metadata enrichment-->
                    </TEI:placeName>
                    <TEI:lb id="d10e15920" n="3"/>
                in der <TEI:placeName ref="#GettyID:1025416 #GettyID:4000934 #GettyID:7244828 #GettyID:7244823 #GettyID:7244820 #GettyID:7244815 #GettyID:7244814 #GettyID:7244811 #GettyID:7244810 #GettyID:7244809 #GettyID:7244808 #GettyID:7244803 #GettyID:7244802 #GettyID:7244915">Au</TEI:placeName>. Zu <TEI:placeName ref="#GettyID:7003991">
                        <TEI:emph>Pesaro</TEI:emph>
                        <!--//Start of metadata enrichment-->
                        <addName xmlns="http://www.tei-c.org/ns/1.0" type="display">Pesaro</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Pesaro</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Pisaurum</addName><!--//End of metadata enrichment-->
                    </TEI:placeName> gefunden<TEI:lb id="d10e15931" n="4"/>
                im alten <TEI:placeName ref="#GettyID:7003125">
                        <TEI:emph>Umbrien</TEI:emph>
                        <!--//Start of metadata enrichment-->
                        <addName xmlns="http://www.tei-c.org/ns/1.0" type="display">Umbria</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Umbria</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Umbrian</addName><!--//End of metadata enrichment-->
                    </TEI:placeName> es ist ein feiner<TEI:lb id="d10e15940" n="5"/>
                    <TEI:emph>Bronze Guß</TEI:emph>. Eine schone <TEI:emph>Basis</TEI:emph>
                    <TEI:lb id="d10e15948" n="6"/>
                    hat es, der aber neu ist vom <TEI:persName ref="#CerlID:cnp00394876" key="#PND:118539086">
                        <TEI:emph>Ghibert</TEI:emph>
                        <!--//Start of metadata enrichment-->
                        <addName xmlns="http://www.tei-c.org/ns/1.0" type="display">Ghiberti, Lorenzo</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Lorenzo Ghiberti</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Ghiverti</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Lorenzo Ghibert</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Ghibert</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Lorenz Ghiberti</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Lorenzo di Cione di Ser Buonaccorso</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Lorenzo</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Lorenzo di Cione di ser Buonaccorso</addName><!--//End of metadata enrichment-->
                    </TEI:persName>
                    <TEI:lb id="d10e15956" n="7"/>
                mit vielen Schnitzwerck. Sie steht<TEI:lb id="d10e15959" n="8"/>
                als hielte sie etwas. <TEI:persName ref="#CerlID:cnp00402479" key="#PND:118964925">
                        <TEI:emph>Gori</TEI:emph>
                        <!--//Start of metadata enrichment-->
                        <addName xmlns="http://www.tei-c.org/ns/1.0" type="display">Gori, Antonio Francesco</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Antonio Francesco Gori</addName>
