                            <addName xmlns="http://www.tei-c.org/ns/1.0">Cypselus</addName><!--//End of metadata enrichment-->
                        </TEI:persName>
                    </TEI:term>, nachher<TEI:lb id="d10e11645" n="23"/>
                aber Keule und <TEI:hi rend="underline">Löwenhaut</TEI:hi> als<TEI:lb id="d10e11650" n="24"/>
                    <TEI:note id="d10e11652" place="margin">die Proportion<TEI:lb id="d10e11654" n="25"/>
                leidet leine Aen<TEI:lb id="d10e11656" n="26"/>
                derung, solte di<TEI:lb id="d10e11658" n="27"/>
                ser Stein nicht<TEI:lb id="d10e11660" n="28"/>
                der Höhe der <TEI:emph>Sta<TEI:lb id="d10e11664" n="29"/>
                tue</TEI:emph> mit denne<TEI:lb id="d10e11668" n="30"/>
                man nicht ein<TEI:lb id="d10e11670" n="31"/>
                Horigunt seyn<TEI:lb id="d10e11672" n="32"/>
                kann liegen</TEI:note>
                    <TEI:lb id="d10e11676" n="33"/>
                    <TEI:note id="d10e11678" place="margin">
                        <TEI:hi rend="underline">trinckend</TEI:hi> Er sef<TEI:lb id="d10e11683" n="34"/>
                einst <TEI:emph>phali</TEI:emph> &amp;<TEI:lb id="d10e11688" n="35"/>
                        <TEI:emph>Centauren</TEI:emph> St<TEI:unclear>  </TEI:unclear>
                        <TEI:lb id="d10e11697" n="36"/>
                aus, womit di<TEI:lb id="d10e11699" n="37"/>
                ser <TEI:emph>
                            <TEI:unclear>t</TEI:unclear>ractiren</TEI:emph>
                        <TEI:lb id="d10e11707" n="38"/>
