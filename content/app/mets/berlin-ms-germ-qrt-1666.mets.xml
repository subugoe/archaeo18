                    <TEI:lb id="d10e5815" n="1"/>
                    2.) <TEI:bibl>
                        <TEI:ref target="#http://arachne.uni-koeln.de/item/buchseite/5730">
                            <TEI:term target="#http://arachne.uni-koeln.de/item/gruppen/400206">no 14.</TEI:term>
                        </TEI:ref>
                    </TEI:bibl> siehe <TEI:bibl>
                        <TEI:ref target="#http://digi.ub.uni-heidelberg.de/diglit/winckelmann1764">
                            <TEI:emph>Winckelmann</TEI:emph> G.d.K.</TEI:ref>
                    </TEI:bibl>
                    <TEI:lb id="d10e5829" n="2"/>
                ist <TEI:emph>Etruscisch.</TEI:emph>
                </TEI:p>
                <TEI:lb id="d10e5836" n="3"/>
                <TEI:p id="d10e5838">3) <TEI:bibl>
