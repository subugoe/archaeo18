                        <addName xmlns="http://www.tei-c.org/ns/1.0">Antonio Francisco Gorius</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Gorius</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Antonius Gorius</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Antonius Franciscus Gorius</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Antonius Franciscus Gorus</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Gorus</addName><!--//End of metadata enrichment-->
                    </TEI:persName> meynt<TEI:lb id="d10e15967" n="9"/>
                es sey ein <TEI:emph>Genius</TEI:emph>. Sie ward bey<TEI:lb id="d10e15972" n="10"/>
                    der Findung Für einen <TEI:persName ref="#CerlID:cnp00579062" key="#PND:118651439">
                        <TEI:emph>Bachhus</TEI:emph>
                        <!--//Start of metadata enrichment-->
                        <addName xmlns="http://www.tei-c.org/ns/1.0" type="display">Bacchus</addName><!--//End of metadata enrichment-->
                    </TEI:persName>
                    <TEI:lb id="d10e15981" n="11"/>
                    gehalten. <TEI:persName ref="#CerlID:cnp01231349" key="#PND:118658115">
                        <TEI:emph>F. Bempo</TEI:emph>
                        <!--//Start of metadata enrichment-->
                        <addName xmlns="http://www.tei-c.org/ns/1.0" type="display">Bembo, Pietro</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Pietro Bembo</addName>
