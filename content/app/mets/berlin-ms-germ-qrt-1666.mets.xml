                        <addName xmlns="http://www.tei-c.org/ns/1.0">Giambologa</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Gian Bologna</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Giovanni</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Giovanni da Bolognese</addName>
                        <addName xmlns="http://www.tei-c.org/ns/1.0">Jean</addName>
