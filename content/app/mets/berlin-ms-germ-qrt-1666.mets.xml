                    <TEI:bibl>
                        <TEI:ref target="#http://archive.org/stream/leantichitadelac00maur#page/n4/mode/1up">
                            <TEI:emph>
                                <TEI:choice>
                                    <TEI:sic>1554.</TEI:sic>
                                    <TEI:corr>1556.</TEI:corr>
                                </TEI:choice> Ulysses Aldroandi Antichita della<TEI:lb id="d10e1583" n="63"/>
                Cita Roma</TEI:emph>
                        </TEI:ref>
