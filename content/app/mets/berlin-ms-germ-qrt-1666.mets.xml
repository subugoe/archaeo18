                    <TEI:emph>
                        <TEI:persName ref="#CerlID:cnp00574918" key="#PND:118639552">Herculus<!--//Start of metadata enrichment-->
                            <addName xmlns="http://www.tei-c.org/ns/1.0" type="display">Herakles</addName>
                            <addName xmlns="http://www.tei-c.org/ns/1.0">Ercole</addName>
