          erster Flor <TEI:date when="-0490">a.° 490.</TEI:date><TEI:lb id="d7115e4410" n="16"/></TEI:p><TEI:p id="d7115e4411"><TEI:emph><TEI:placeName ref="#GettyID:1042201">Samos<!--//Start of metadata enrichment--><addName type="display">Sámos</addName><addName>Sámos</addName><addName>Samos</addName><addName>Vathy harbor</addName><addName>Vathy</addName><addName>Port Vathi</addName><addName>Limín Vathéos</addName><addName>Limén Vathéos</addName><addName>Liman Vathéos</addName><addName>Tigani</addName><!--//End of metadata enrichment--></TEI:placeName>, <TEI:placeName ref="#GettyID:6002765">Ionien<!--//Start of metadata enrichment--><addName type="display">Ionia</addName><addName>Ionia</addName><!--//End of metadata enrichment--></TEI:placeName></TEI:emph> blüht<TEI:expan>en</TEI:expan> früher.<TEI:lb id="d7115e4422" n="17"/></TEI:p><TEI:p id="d7115e4423">Von der ältsten Kunst <TEI:placeName ref="#GettyID:1000074">Griechenlands<!--//Start of metadata enrichment--><addName type="display">Ellás</addName><addName>Ellás</addName><addName>Ellas</addName><addName>Ellinikí Dimokratía</addName><addName>Elliniki Dimokratia</addName><addName>Elliniki Dhimokratia</addName><addName>Greece</addName><addName>Hellenic Republic</addName><addName>Grèce</addName><addName>République hellénique</addName><addName>Griechenland</addName><addName>Grecia</addName><addName>Griekenland</addName><addName>Grécia</addName><addName>GRC</addName><addName>GR</addName><addName>ISO300</addName><addName>GR00</addName><addName>Kingdom of Greece</addName><addName>Hellas</addName><addName>Greek</addName><addName>Hellenic</addName><!--//End of metadata enrichment--></TEI:placeName> haben wir Nachricht<TEI:lb id="d7115e4428" n="18"/>
          
