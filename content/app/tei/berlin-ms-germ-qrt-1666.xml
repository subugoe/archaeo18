                <TEI:note place="margin">
                        <TEI:emph>Hercules</TEI:emph>
                    </TEI:note>
                    <TEI:lb/>
                <TEI:note place="margin">blos eine <TEI:term>andre<TEI:lb/>
                Vorstellung selbi-<TEI:lb/>
                gen Süsjetts</TEI:term>.</TEI:note>
